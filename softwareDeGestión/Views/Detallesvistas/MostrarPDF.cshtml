@{
    string? rutaPDF = ViewData["RutaPDF"] as string;
    ViewData["Title"] = "PDF.";
}

@if (!string.IsNullOrEmpty(rutaPDF))
{
    <div class="position-fixed start-0 top-0 end-0 bottom-0" >
        <embed src="@rutaPDF" style="width: 100%; height:100%; " type="application/pdf" />
        
    </div>
}
else
{
    <p>No se encontró el archivo PDF.</p>
}
